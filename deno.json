{
  "nodeModulesDir": "auto",
  "tasks": {
    "convert": "deno run --allow-read --allow-write cli/main.ts",
    "test": "deno test --allow-read --allow-write cli/",
    "lint": "deno run -A npm:@biomejs/biome@1.9.4 lint .",
    "fmt": "deno run -A npm:@biomejs/biome@1.9.4 format --write .",
    "check": "deno run -A npm:@biomejs/biome@1.9.4 check .",
    "dev": "deno run -A npm:vite",
    "build": "deno run -A npm:vite build",
    "preview": "deno run -A npm:vite preview"
  },
  "imports": {
    "pdfjs-dist": "npm:pdfjs-dist@4.9.155",
    "vue": "npm:vue@3",
    "@vitejs/plugin-vue": "npm:@vitejs/plugin-vue@5",
    "vite": "npm:vite@6",
    "pdf-lib": "npm:pdf-lib@1.17.1",
    "fflate": "npm:fflate@0.8.2",
    "vue-router": "npm:vue-router@4"
  }
}
